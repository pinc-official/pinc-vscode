{
  "name": "pinc",
  "displayName": "pinc",
  "description": "The official Plugin for the PiNC CMS Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pinc",
        "aliases": [
          "PINC",
          "PiNC",
          "pinc"
        ],
        "extensions": [
          ".pi"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pinc",
        "scopeName": "source.pi",
        "path": "./syntaxes/pinc.tmLanguage.json"
      }
    ]
  }
}
